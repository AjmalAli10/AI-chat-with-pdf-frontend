{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ai-chat-with-pdf-backend.vercel.app/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://ai-chat-with-pdf-backend.vercel.app/uploads/:path*"
    }
  ],
  "routes": [
    {
      "src": "/pdf.worker.min.js",
      "dest": "/pdf.worker.min.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/vite.svg",
      "dest": "/vite.svg"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_NODE_ENV": "production"
  }
}
